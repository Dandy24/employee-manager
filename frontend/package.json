{
  "name": "frontend",
  "version": "1.1.1",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test --maxWorkers=50%",
    "test:coverage": "react-scripts test --coverage",
    "test:ci": "react-scripts test --runInBand",
    "test:e2e": "./node_modules/.bin/cypress open",
    "test:e2e:ci": "./node_modules/.bin/cypress run --headless --browser chrome",
    "ci": "REACT_APP_TEST_ENV=true start-server-and-test start http://localhost:3000 test:e2e:ci",
    "lighthouse": "lighthouse http://localhost:3000 --preset=desktop --output-path=./lighthouse-reports/lighthouse.html",
    "lighthouse:ci": "start-server-and-test serve http://localhost:3000 lighthouse",
    "serve": "serve -s build",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.12.1",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@percy/cli": "^1.0.0-beta.68",
    "@percy/cypress": "^3.1.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^9.5.1",
    "cypress-file-upload": "^6.0.0-beta.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.14.0",
    "ts-loader": "^9.2.4",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.15",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-table": "^7.7.7",
    "antd": "^4.16.8",
    "css-loader": "^6.2.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "cypress-real-events": "^1.7.0",
    "formik": "^2.2.9",
    "formik-antd": "^2.0.1",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "recharts": "^2.1.6",
    "style-loader": "^3.2.1",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.9"
  },
  "jest": {
    "resetMocks": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/src/models/"
    ]
  },
  "nyc": {
    "exclude" : ["src/models/**"]
  }
}
